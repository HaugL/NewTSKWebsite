<div class = 'form-container'>
	<div class = 'title'>Create a Category</div>
	 <%= semantic_form_for @category, :html => {:multipart => true} do |f| %>
	    <%= f.inputs do %>
	      <%= f.input :name, :required => true%>
	    <% end %>
	    <%= f.actions do %>
	      <%= f.action :submit, :as => :button %>
	    <% end %>
	  <% end %>
</div>
