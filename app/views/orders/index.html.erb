
<table class = 'table'>
	  <thead>
		<tr>
			<th> Order Id </th>
			<th>Cost</th>
			<th></th>
			<th>Date Ordered</th>
		</tr>
	  </thead>
		<% @orders.each do |order| %>
		<tr>
				
				<td><%= link_to order.id, order_path(order)%></td>
				<td>$ <%= number_with_precision(order.cost, :precision => 2) %></td>
				<td></td>
				<td><%= order.created_at.strftime('%b %d, %Y')  %></td>
		
		</tr>
			<% end %>

	</table>